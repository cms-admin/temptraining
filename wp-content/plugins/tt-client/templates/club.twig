<section class="club-brand" {% if club.bg.counters %}style="background-image: url({{function('wp_get_attachment_image_url', club.bg.counters, 'original')}});"{% endif %}>

  <svg class="club-brand__wave" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="0 0 1920 140" preserveAspectRatio="xMidYMid meet">
    <path fill-rule="evenodd"  d="M-0.000,59.151 C-0.000,59.151 359.057,-73.917 685.316,59.151 C1118.287,235.743 1920.000,59.151 1920.000,59.151 L1920.000,140.000 L-0.000,140.000 L-0.000,59.151 Z"/>
  </svg>

  <div class="container">
    <div class="grid grid--align-center grid--justify-center">
      <div class="grid__col-xxs-12 grid__col-sm-10 grid__col-md-10 grid__col-lg-8">
        <figure class="club-brand__logo">
          <span>{{__('КЛУБ', 'tt-client')}}</span>
          <span>{{__('ТЕМП', 'tt-client')}}</span>
        </figure>
      </div>
    </div>

    <div class="club-brand__counter-members">
      <span>{{__('Нас уже', 'tt-client')}}</span>
      <figure id="counter_members" data-countup="" data-countup-options='{
        "start": 0,
        "end": {{count_members}},
        "decimals": 0,
        "duration": 1
      }'>0</figure>
      <span>{{count_members_str}}</span>
    </div>

    <div class="club-brand__counter-money">
      <span>{{__('В фонде', 'tt-client')}}</span>
      <figure id="counter_money" data-countup="" data-countup-options='{
        "start": 0,
        "end": {{count_money}},
        "decimals": 0,
        "duration": 1
      }'>0</figure>
      <span>{{count_money_str}}</span>
    </div>
  </div>
  
  
</section>

<section class="club-intro">
  <div class="container">
    <h2 class="club-intro__title">{{club.intro.title}}</h2>

    <div class="grid">
      {% set intro = {'1': 'stopwatch', '2': 'teamwork', '3': 'motivation', '4': 'email'} %}
      {% for item in intro_text %}
        <div class="grid__col-sm-6 grid__col-md-3">
          <div class="club-intro__item">
            <figure class="club-intro__item-icon">
              <i class="clubicon-{{ intro[loop.index] }}"></i>
            </figure>
            <div class="club-intro__item-text">{{item}}</div>
          </div>
        </div>
      {% endfor %}
    </div>

    <button class="club-intro__button" data-action="membership" rel="club_membership">
      <span>{{__('Мы представляем вам клуб ТЕМП', 'tt-client')}}</span>
    </button>
  </div>
  
</section>

<section class="club-target">
  <div class="container">
    <div class="grid">
      <div class="grid__col-xxs-12 grid__col-md-6">
        <div class="grid grid--align-center">
          <div class="grid__col-xxs-4 grid__col-md-12">
            <figure class="club-target__logo">
              <i class="clubicon-target"></i>
            </figure>
          </div>
          <div class="grid__col-xxs-8 grid__col-md-12">
            <h3 class="club-target__title">Цели клуба</h3>
          </div>
        </div>
        
        <div class="club-target__text">{{club.target.text}}</div>
        <div>
          <button class="club-target__button" data-action="membership" rel="club_membership">
            <span>{{__('Вступить в клуб', 'tt-client')}}</span>
          </button>
        </div>
        
      </div>

      <div class="grid__col-md-6">
        <div class="club-target__list">
          <div class="grid grid--justify-end">
            <div class="grid__col-sm-4 grid__col-md-6">
              <div class="club-target__item is-first">
                <figure class="club-target__item-icon">
                  <i class="clubicon-contract"></i>
                </figure>
                <h4 class="club-target__item-title">{{club.target.list1_title}}</h4>
                <div class="club-target__item-text">{{club.target.list1_text}}</div>
              </div>
            </div>

            <div class="grid__col-sm-4 grid__col-md-6">
              <div class="club-target__item is-second">
                <figure class="club-target__item-icon">
                  <i class="clubicon-profits-1"></i>
                </figure>
                <h4 class="club-target__item-title">{{club.target.list2_title}}</h4>
                <div class="club-target__item-text">{{club.target.list2_text}}</div>
              </div>
            </div>

            <div class="grid__col-sm-4 grid__col-md-6">
              <div class="club-target__item is-third">
                <figure class="club-target__item-icon">
                  <i class="clubicon-presentation"></i>
                </figure>
                <h4 class="club-target__item-title">{{club.target.list3_title}}</h4>
                <div class="club-target__item-text">{{club.target.list3_text}}</div>
              </div>
            </div>

          </div>
        </div>
      </div>
    </div>
  </div>
</section>

